						<div class="home_banner_wrapper">
							<div class="container">
								<div class="row">
									<div class="home_banner_inner">
										<div class="home_banner_content">
											<div class="col-sm-12 banner_item">
                                                    {% for ad in ads %}
                                                    {% if ad.area == 'home_1' %}
                                                                                                        
                                                            {% if ad.htmlcode is not empty %}
                                                                {% autoescape %}
                                                                    {{ad.htmlcode|raw}}
                                                                {% endautoescape %}
                                                            {% endif %}

