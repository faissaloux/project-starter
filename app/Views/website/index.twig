			</div>
			
			<div class="shopify-section index-section index-section-prosli">
				<div>
					<section class="home_prosli_layout">
						<div class="home_prosli_wrapper">
							<div class="container">
								<div class="row">
									<div class="prosli_group">
										<div class="page-title">
											<h2>{{ 'BLOCK_NAME_1'|options }}</h2>
										</div>
										<div class="home_prosli_inner">
											<div class="home_prosli_content">
											{% for i in 0..11 %}

												<div class="content_product col-sm-2">
													<div class="row-container product list-unstyled clearfix">
														<div class="row-left">
                                                                    
																		<a href="{{ path_for('website.product', {'id': products[i].id}) }}" class="hoverBorder container_item">
																			<div class="hoverBorderWrapper">
																				<img src="{{url.media}}{{ products[i].ThumbnailsImages().1}}" class="not-rotation img-responsive front" >
																				<div class="mask"></div>
																				<img src="{{url.media}}{{ products[i].ThumbnailsImages().2}}" class="rotation img-responsive" >
																			</div>
																		</a>
																		<span class="sale_banner">
																<span class="sale_text">{{products[i].discounte()}}</span>
															</span>
																		<div class="hover-mask">
																			<div class="group-mask">
																				<div class="inner-mask">
																					<div class="group-actionbutton">
																						<ul class="quickview-wishlist-wrapper">
																							<li class="wishlist hidden-xs">
																								<a class="wish-list"><span class="hidden-xs"><i class="fa fa-heart" title="Wishlist"></i></span></a>
																							</li>
																						</ul>
																					</div>
																				</div>
																			</div>
																		</div>
																	</div>
																	<div class="row-right animMix">
																		<div class="product-title"><a class="title-5" href="{{ path_for('website.product', {'id': products[i].id}) }}">{{products[i].name}}</a></div>
																		<div class="product-price">
																			<span class="price_sale">{{products[i].discount_price}}$</span>
																			<del class="price_compare">{{products[i].price}}$</del>
																		</div>
																	</div>
													</div>
												</div>
												{% endfor %}
											</div>
										</div>
									</div>
								</div>
							</div>
						</div>
					</section>
				</div>
			</div>

			
			
			
			
			
			
			
			
			
			
			
			
			
			
			
